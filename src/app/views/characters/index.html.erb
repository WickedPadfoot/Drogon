<p id="notice"><%= notice %></p>

<h1>Listing Characters</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @characters.each do |character| %>
      <%= render partial: "list", object: character %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Character', new_character_path %>